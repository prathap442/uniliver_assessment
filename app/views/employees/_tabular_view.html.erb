<table border="2">
  <thead>
    <th>EmpId</th>
    <th>Name</th>
    <th>Description</th>
    <th>Age</th>
    <th>Salary</th>
  </thead>
  <tbody>
    <%employees.each do |emp|%>
      <tr>
        <td><%= emp.emp_id%></td>
        <td><%= emp.name%></td>
        <td><%= emp.description%></td>
        <td><%= emp.age%></td>
        <td><%= emp.salary%></td>
      </tr>
    <%end%>
  </tbody>
</table>